{
  "name": "@holaluz/margarita",
  "version": "0.0.0-semantically-released",
  "repository": {
    "type": "git",
    "url": "https://github.com/holaluz/margarita.git"
  },
  "main": "dist/margarita.umd.js",
  "unpkg": "dist/margarita.umd.min.js",
  "browser": "dist/margarita.common.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vue-cli-service build --target lib --name margarita src/index.js",
    "predeploy": "npm run storybook:build",
    "deploy": "gh-pages -d storybook-static",
    "format": "prettier 'src/**/*.{js,vue}' --write && stylelint 'src/**/*.scss' --fix",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint 'src/**/*.scss'",
    "lint:js": "eslint 'src/**/*.{js,vue}'",
    "prepublishOnly": "npm it && npm run build",
    "semantic-release": "semantic-release",
    "start": "npm run storybook:serve",
    "storybook:build": "vue-cli-service storybook:build -c .storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c .storybook",
    "test": "vue-cli-service test:unit",
    "test:watch": "vue-cli-service test:unit --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "sass-mq": "^5.0.0",
    "snarkdown": "^2.0.0",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@holaluz/npm-scripts": "^5.0.1",
    "@storybook/addon-a11y": "^6.1.9",
    "@storybook/addon-actions": "^6.1.9",
    "@storybook/addon-knobs": "^6.1.9",
    "@storybook/addon-links": "^6.1.9",
    "@storybook/addon-viewport": "^6.1.9",
    "@storybook/vue": "^6.1.9",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/vue": "^5.6.1",
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-eslint": "^4.5.9",
    "@vue/cli-plugin-unit-jest": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.6.3",
    "copy-webpack-plugin": "^6.3.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-testing-library": "^3.10.1",
    "eslint-plugin-vue": "^7.1.0",
    "gh-pages": "^3.1.0",
    "highlight-loader": "^0.7.4",
    "html-loader": "^1.3.2",
    "husky": "^4.3",
    "is-vue-component": "^1.0.0",
    "jest": "^26.6.3",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.2",
    "lodash.camelcase": "^4.3.0",
    "lodash.merge": "^4.6.2",
    "lodash.shuffle": "^4.2.0",
    "markdown-loader": "^6.0.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.0",
    "semantic-release": "^17.3.0",
    "semantic-release-slack-bot": "^1.7.0",
    "stylelint": "^13.8.0",
    "stylelint-config-concentric-order": "^4.0.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.1.0",
    "vue-cli-plugin-storybook": "^2.0.0",
    "vue-functional-data-merge": "^3.1.0",
    "vue-template-compiler": "^2.6.12"
  },
  "publishConfig": {
    "access": "public"
  },
  "jsDelivr": "dist/margarita.umd.min.js"
}
