{
  "name": "margarita",
  "version": "0.2.0",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "prepare": "npm run build",
    "build": "node_modules/.bin/webpack",
    "build:storybook": "build-storybook",
    "dev": "start-storybook -p 6006",
    "docker:down": "docker-compose down --volumes",
    "docker:start": "docker-compose run -p 6006:6006 storybook && npm run docker:down",
    "docker:test": "docker-compose run tests && npm run docker:down",
    "start": "npm run dev",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint --ext .js,.vue src test/unit",
    "lint:scss": "sass-lint 'src/**/*.scss' -v -q",
    "test": "npm run lint && npm run unit",
    "test:watch": "npm run unit:watch",
    "unit": "jest --config test/unit/jest.conf.js",
    "unit:watch": "jest --watchAll --config test/unit/jest.conf.js"
  },
  "dependencies": {
    "vue": "^2.5.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.3",
    "@storybook/addon-knobs": "^3.4.3",
    "@storybook/addon-links": "^3.4.3",
    "@storybook/addon-notes": "^3.4.3",
    "@storybook/addon-viewport": "^3.4.3",
    "@storybook/addons": "^3.4.3",
    "@storybook/vue": "^3.4.3",
    "@vue/test-utils": "^1.0.0-beta.16",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^21.0.2",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "highlight-loader": "^0.7.2",
    "husky": "^1.0.0-rc.9",
    "jest": "^22.0.4",
    "jest-serializer-vue": "^0.3.0",
    "lodash": "^4.17.10",
    "node-sass": "^4.9.2",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.0.1",
    "vue-jest": "^1.0.2",
    "vue-loader": "^14.2.2",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.16"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  }
}
