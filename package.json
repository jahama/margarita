{
  "name": "@holaluz/margarita",
  "version": "0.0.0-semantically-released",
  "main": "dist/margarita.umd.js",
  "browser": "dist/margarita.common.js",
  "unpkg": "dist/margarita.umd.min.js",
  "jsDelivr": "dist/margarita.umd.min.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepublishOnly": "npm i && npm run build",
    "build": "vue-cli-service build --target lib --name margarita src/index.js",
    "deploy": "gh-pages -d storybook-static",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint 'src/**/*.{js,vue}'",
    "lint:css": "stylelint 'src/**/*.scss'",
    "format": "prettier 'src/**/*.{js,vue}' --write && stylelint 'src/**/*.scss' --fix",
    "predeploy": "npm run storybook:build",
    "start": "npm run storybook:serve",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "test": "vue-cli-service test:unit",
    "test:watch": "vue-cli-service test:unit --watch",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "sass-mq": "^5.0.0",
    "snarkdown": "^1.2.2",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@holaluz/npm-scripts": "^4.0.1",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/vue": "^5.0.4",
    "@vue/cli-plugin-babel": "^4.5.2",
    "@vue/cli-plugin-eslint": "^4.5.2",
    "@vue/cli-plugin-unit-jest": "^4.5.2",
    "@vue/cli-service": "^4.5.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.3.0",
    "copy-webpack-plugin": "^6.0.3",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-testing-library": "^2.2.3",
    "eslint-plugin-vue": "^6.2.2",
    "gh-pages": "^3.1.0",
    "highlight-loader": "^0.7.4",
    "html-loader": "^1.1.0",
    "husky": "^3.1.0",
    "is-vue-component": "^1.0.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^10.2.11",
    "lodash.camelcase": "^4.3.0",
    "lodash.shuffle": "^4.2.0",
    "markdown-loader": "^5.1.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "sass-loader": "^9.0.3",
    "semantic-release": "^17.1.1",
    "stylelint": "^13.6.1",
    "stylelint-config-concentric-order": "^4.0.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.1.0",
    "vue-cli-plugin-storybook": "^0.6.0",
    "vue-functional-data-merge": "^3.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/holaluz/margarita.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
