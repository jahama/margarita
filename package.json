{
  "name": "@holaluz/margarita",
  "version": "0.0.0-semantically-released",
  "main": "dist/margarita.umd.js",
  "browser": "dist/margarita.common.js",
  "unpkg": "dist/margarita.umd.min.js",
  "jsDelivr": "dist/margarita.umd.min.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "vue-cli-service build --target lib --name margarita src/index.js",
    "deploy": "gh-pages -d storybook-static",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint 'src/**/*.{js,vue}'",
    "lint:css": "stylelint 'src/**/*.scss'",
    "format": "prettier 'src/**/*.{js,vue}' --write && stylelint 'src/**/*.scss' --fix",
    "predeploy": "npm run storybook:build",
    "start": "npm run storybook:serve",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "test": "vue-cli-service test:unit",
    "test:watch": "vue-cli-service test:unit --watch",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "sass-mq": "^5.0.0",
    "snarkdown": "^1.2.2",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@holaluz/npm-scripts": "^1.1.0",
    "@storybook/addon-a11y": "^5.1.11",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-notes": "^5.1.11",
    "@storybook/addon-viewport": "^5.1.11",
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/vue": "^2.1.0",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^23.6.0",
    "copy-webpack-plugin": "^5.0.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-testing-library": "^1.3.1",
    "eslint-plugin-vue": "^5.1.0",
    "gh-pages": "^2.1.1",
    "highlight-loader": "^0.7.3",
    "html-loader": "^0.5.5",
    "husky": "^3.0.4",
    "is-vue-component": "^1.0.0",
    "jest-watch-typeahead": "^0.4.0",
    "lint-staged": "^9.2.5",
    "lodash.camelcase": "^4.3.0",
    "markdown-loader": "^5.1.0",
    "node-sass": "^4.9.0",
    "prettier": "^1.17.0",
    "sass-loader": "^7.3.1",
    "semantic-release": "^15.13.24",
    "stylelint": "^10.0.1",
    "stylelint-config-concentric-order": "^3.4.1",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-prettier": "^1.1.0",
    "vue-cli-plugin-storybook": "^0.6.1",
    "vue-functional-data-merge": "^2.0.7",
    "vue-template-compiler": "^2.6.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/holaluz/margarita.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
